;; Testing each operand type to see if they are encoded properly

;; ================ One Operand
; Const
int 255

; Reg
pop r1
push.s r1a

; [Reg]
push [r3]
pop.s [r5]

; [Reg + Off]
lom [r7 + 255]

;; ================ Two Operands
; Const Reg
or 255, r1
and.s 255, r1a

; Reg Reg
nor r3, r5

; [Reg] Reg
nand [r7], r9

; [Reg + Off] Reg
xnor [r11 + 255], r13

; [Reg] [Reg + Off]
cmp [r0], [r0 + 255]
test.s [r1], [r1 - 1]
